<div id="ModalRamos" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog"  role="document">
        <div class="modal-content">
          <div class="modal-header">
              <h5>Consulta de Ramos</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="false">&times;</span>
              </button>
          </div>
          <div class="modal-body">
              <div class="form-group">
                  <label>Clave</label>
                  <input id="txtclaveRamo" name="txtClaveRamo" class="form-control" placeholder="Texto" />
              </div>
              <div class="form-group">
                  <label>Descripcion</label>
                  <input id="txtDescripcionRamo" name="txtxDescripcionRamo" class="form-control" placeholder="Texto" />
              </div>
              <div class="table-responsive">
                  <table class="table table-striped table-bordered table-hover">
                      <thead style="background-color: silver">
                          <tr>
                              <td>Clave</td>
                              <td>Descripcion</td>
                          </tr>
                      </thead>
                      <tbody id="tablaRamos">
                      </tbody>
                  </table>
              </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-primary" id="btnBuscarRamo">Buscar</button>
          </div>
        </div>
    </div>
</div>